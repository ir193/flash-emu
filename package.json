{
  "name": "flash-emu",
  "version": "2.0.1",
  "description": "ActionScript Virtual Machine 2",
  "main": "./dist/flashemu.js",
  "types": "flashemu.d.ts",
  "scripts": {
    "dev": "rollup -w -c ./build/dev.rollup.config.js -o dist/flashemu.js",
    "build": "rollup -c ./build/build.rollup.config.js -o dist/flashemu.js",
    "cmd": "cmd",
    "test": "mocha --require ./test-require ./test/**/*.test.js",
    "watch": "mocha --watch --require ./test-require ./test/**/*.test.js",
    "t": "node ./test/dev/test.js",
    "build-lib": "node build/build.js",
    "tsc": "tsc --traceResolution -p ."
  },
  "keywords": [
    "avm2",
    "actionscript",
    "flash",
    "swf"
  ],
  "files": [
    "dist/*.js",
    "dist/*.map",
    "lib/*.abc",
    "src/",
    "typeings/",
    "package.json",
    "tsconfig.json",
    "tslint.json",
    "LICENSE",
    "flashemu.d.ts"
  ],
  "author": "imspace",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/spacemeowx2/flash-emu"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.19",
    "@types/pako": "^1.0.0",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^3.14.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "espower-loader": "^1.2.2",
    "espower-typescript": "^8.0.2",
    "mocha": "^3.5.0",
    "power-assert": "^1.4.4",
    "rollup": "^0.45.2",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "source-map-support": "^0.4.11",
    "thunk-mocha": "^1.0.8",
    "ts-node": "^3.3.0",
    "tslint": "^4.5.1",
    "tslint-config-standard": "^3.0.0",
    "typescript": "^2.4.1",
    "uglify-es": "^3.0.28"
  },
  "dependencies": {
    "pako": "^1.0.5"
  }
}
